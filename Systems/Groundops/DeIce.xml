<PropertyList>

    <!-- Convert De-Ice-truck movement to roll speed in meter per second-->

    <filter>
        <name>Truck</name>
        <type>gain</type>
        <gain>1</gain>
        <enable>
            <condition>
                <not-equals>
                    <property>services/deicing_truck/truck[0]/direction</property>
                    <value>0.0</value>
                </not-equals>
            </condition>
        </enable>
        <input>
            <expression>
                <table>
                    <property>services/deicing_truck/truck[0]/position-norm</property>
                    <entry>
                        <ind>0.0</ind>
                        <dep> 0.00</dep>
                    </entry>
                    <entry>
                        <ind>0.01</ind>
                        <dep>0.75</dep>
                    </entry>
                    <entry>
                        <ind>0.19</ind>
                        <dep>0.75</dep>
                    </entry>
                    <entry>
                        <ind>0.20</ind>
                        <dep>0.00</dep>
                    </entry>
                    <entry>
                        <ind>0.30</ind>
                        <dep>0.00</dep>
                    </entry>
                    <entry>
                        <ind>0.31</ind>
                        <dep>0.75</dep>
                    </entry>
                    <entry>
                        <ind>0.49</ind>
                        <dep>0.75</dep>
                    </entry>
                    <entry>
                        <ind>0.50</ind>
                        <dep>0.00</dep>
                    </entry>
                    <entry>
                        <ind>1.00</ind>
                        <dep>0.00</dep>
                    </entry>
                </table>
            </expression>
        </input>
        <output>services/deicing_truck/truck[0]/speed-mps</output>
    </filter>

