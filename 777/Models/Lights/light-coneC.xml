<?xml version="1.0"?>

<PropertyList>

 <path>light-coneC.ac</path>

    <animation>
        <type>select</type>
        <object-name>LandingLightCone_C</object-name>
        <condition>
            <and>
                <property>sim/rendering/rembrandt/enabled</property>
                <greater-than>
                    <property>sim/multiplay/generic/int[4]</property>
                    <value>5</value>
                </greater-than>
                <greater-than>
                    <property>systems/electrical/L-XFR</property>
                    <value>80</value>
                </greater-than>
                <equals>
                    <property>gear/gear[0]/position-norm</property>
                    <value>1.0</value>
                </equals>
            </and>
        </condition>
    </animation>

    <animation>
        <type>light</type>
        <light-type>spot</light-type>
        <name>LandingLight_C</name>
        <object-name>LandingLightCone_C</object-name>
        <nopreview/>
        <position>
            <x>-19.579</x>
            <y>0</y>
            <z>-2.831</z>
        </position>
        <direction>
            <x>-29</x>
            <y>0</y>
            <z>-3.6</z>
        </direction>
        <ambient>
            <r>0.15</r>
            <g>0.15</g>
            <b>0.15</b>
            <a>1.0</a>
        </ambient>
        <diffuse>
            <r>0.7</r>
            <g>0.7</g>
            <b>0.6</b>
            <a>1.0</a>
        </diffuse>
        <specular>
            <r>0.8</r>
            <g>0.8</g>
            <b>0.8</b>
            <a>1.0</a>
        </specular>
        <attenuation>
            <c>1.0</c>
            <l>0.002</l>
            <q>0.00005</q>
        </attenuation>
        <exponent>30.0</exponent>
        <cutoff>100</cutoff>
        <near-m>2</near-m>
        <far-m>1000</far-m>
    </animation>

    <animation>
        <type>select</type>
        <object-name>TaxiLightCone</object-name>
        <condition>
            <and>
                <property>sim/rendering/rembrandt/enabled</property>
                <greater-than>
                    <property>systems/electrical/outputs/taxi-lights</property>
                    <value>5</value>
                </greater-than>
                <greater-than>
                    <property>systems/electrical/L-XFR</property>
                    <value>80</value>
                </greater-than>
                <equals>
                    <property>gear/gear[0]/position-norm</property>
                    <value>1.0</value>
                </equals>
            </and>
        </condition>
    </animation>

    <animation>
        <type>light</type>
        <light-type>spot</light-type>
        <name>TaxiLight</name>
        <object-name>TaxiLightCone</object-name>
        <nopreview/>
        <position>
            <x>-19.579</x>
            <y>0</y>
            <z>-2.831</z>
        </position>
        <direction>
            <x>-29</x>
            <y>0</y>
            <z>-3.6</z>
        </direction>
        <ambient>
            <r>0.15</r>
            <g>0.15</g>
            <b>0.15</b>
            <a>1.0</a>
        </ambient>
        <diffuse>
            <r>0.7</r>
            <g>0.7</g>
            <b>0.6</b>
            <a>1.0</a>
        </diffuse>
        <specular>
            <r>0.8</r>
            <g>0.8</g>
            <b>0.8</b>
            <a>1.0</a>
        </specular>
        <attenuation>
            <c>1.0</c>
            <l>0.0</l>
            <q>0.0001</q>
        </attenuation>
        <exponent>100.0</exponent>
        <cutoff>100</cutoff>
        <near-m>2</near-m>
        <far-m>1000</far-m>
    </animation>
    
    <!-- COMPOSITOR LIGHTS -->
    
    <light>
      <name>LandingLight_C-Compositor</name>
      <type>spot</type>
      <position>
          <x-m>-19.579</x-m>
          <y-m>0</y-m>
          <z-m>-2.831</z-m>
      </position>
      <direction>
          <x>-29</x>
          <y>0</y>
          <z>-3.6</z>
      </direction>
      <ambient>
          <r>0.15</r>
          <g>0.15</g>
          <b>0.15</b>
          <a>1.0</a>
      </ambient>
      <diffuse>
          <r>0.7</r>
          <g>0.7</g>
          <b>0.6</b>
          <a>1.0</a>
      </diffuse>
      <specular>
          <r>0.8</r>
          <g>0.8</g>
          <b>0.8</b>
          <a>1.0</a>
      </specular>
      <attenuation>
          <c>0.5</c>
          <l>0.002</l>
          <q>0.00005</q>
      </attenuation>
      <spot-exponent>30</spot-exponent>
      <spot-cutoff>60</spot-cutoff>
      <range-m>2500</range-m>
    </light>
    
    <animation>
        <type>select</type>
        <object-name>LandingLight_C-Compositor</object-name>
        <condition>
            <and>
                <greater-than>
                    <property>sim/multiplay/generic/int[5]</property>
                    <value>5</value>
                </greater-than>
                <greater-than>
                    <property>systems/electrical/L-XFR</property>
                    <value>80</value>
                </greater-than>
                <equals>
                    <property>gear/gear[0]/position-norm</property>
                    <value>1.0</value>
                </equals>
            </and>
        </condition>
    </animation>
    
    <light>
      <name>TaxiLight-Compositor</name>
      <type>spot</type>
      <position>
          <x-m>-19.579</x-m>
          <y-m>0</y-m>
          <z-m>-2.831</z-m>
      </position>
      <direction>
          <x>-29</x>
          <y>0</y>
          <z>-3.6</z>
      </direction>
      <ambient>
          <r>0.15</r>
          <g>0.15</g>
          <b>0.15</b>
          <a>1.0</a>
      </ambient>
      <diffuse>
          <r>0.7</r>
          <g>0.7</g>
          <b>0.6</b>
          <a>1.0</a>
      </diffuse>
      <specular>
          <r>0.8</r>
          <g>0.8</g>
          <b>0.8</b>
          <a>1.0</a>
      </specular>
      <attenuation>
          <c>0.5</c>
          <l>0.01</l>
          <q>0.0001</q>
      </attenuation>
      <spot-exponent>100</spot-exponent>
      <spot-cutoff>21.5</spot-cutoff>
      <range-m>2500</range-m>
    </light>
    
    <animation>
        <type>select</type>
        <object-name>TaxiLight-Compositor</object-name>
        <condition>
            <and>
                <greater-than>
                    <property>sim/multiplay/generic/int[2]</property>
                    <value>5</value>
                </greater-than>
                <greater-than>
                    <property>systems/electrical/L-XFR</property>
                    <value>80</value>
                </greater-than>
                <equals>
                    <property>gear/gear[0]/position-norm</property>
                    <value>1.0</value>
                </equals>
            </and>
        </condition>
    </animation>
</PropertyList>

