<?xml version="1.0"?>

<!-- 777 Autopilot -->
<!-- Copyright (c) 2022 Josh Davidson (Octal450) -->
<!-- Based off IT-AUTOFLIGHT Technology -->

<PropertyList>
	
	<!-- Misc Logic -->
	<filter>
		<name>Tiller Control Filter Nose</name>
		<type>noise-spike</type>
		<max-rate-of-change>0.75</max-rate-of-change>
		<input>/controls/gear/nosegear-steering-cmd</input>
		<output>/controls/gear/nosegear-steering-cmd-norm</output>
	</filter>

	<filter>
		<name>Tiller Control Filter Main</name>
		<type>noise-spike</type>
		<max-rate-of-change>0.75</max-rate-of-change>
		<input>/controls/gear/maingear-steering-cmd</input>
		<output>/controls/gear/maingear-steering-cmd-norm</output>
	</filter>

	<logic>
		<name>Aircraft On Ground Logic</name>
		<update-interval-secs>0.1</update-interval-secs>
		<input>
			<less-than>
				<property>/position/gear-agl-ft</property>
				<value>2.0</value>
			</less-than>
		</input>
		<output>
			<property>/gear/on-ground</property>
		</output>
	</logic>
	
	<!-- Autopilot Helpers/Calculators -->
	<filter>
		<name>Heading Input</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<condition>
				<equals>
					<property>/systems/navigation/hdgref/reference</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/orientation/heading-deg</property>
		</input>
		<input>/orientation/heading-magnetic-deg</input>
		<output>/autopilot/internal/heading</output>
	</filter>
	
	<filter>
		<name>Track Input</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<condition>
				<equals>
					<property>/systems/navigation/hdgref/reference</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/orientation/track-deg</property>
		</input>
		<input>/orientation/track-magnetic-deg</input>
		<output>/autopilot/internal/track</output>
	</filter>
	
	<filter>
		<name>Heading Error Deg</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<condition>
				<or>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>HDG SEL</value>
					</equals>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>HDG HOLD</value>
					</equals>
				</or>
			</condition>
			<property>/autopilot/settings/heading-bug-deg</property>
			<offset>
				<property>/autopilot/internal/heading</property>
				<scale>-1.0</scale>
			</offset>
		</input>
		<input>
			<condition>
				<or>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>TRK SEL</value>
					</equals>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>TRK HOLD</value>
					</equals>
				</or>
			</condition>
			<property>/autopilot/settings/heading-bug-deg</property>
			<offset>
				<property>/autopilot/internal/track</property>
				<scale>-1.0</scale>
			</offset>
		</input>
		<input>
			<condition>
				<and>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>HDG SEL</value>
					</not-equals>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>HDG HOLD</value>
					</not-equals>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>TRK SEL</value>
					</not-equals>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>TRK HOLD</value>
					</not-equals>
				</and>
			</condition>
			<value>0</value>
		</input>
		<output>/autopilot/internal/heading-error-deg</output>
		<period>
			<min>-180</min>
			<max>180</max>
		</period>
	</filter>
	
	<filter>
		<name>VOR/LOC Error Deg</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<property>/instrumentation/nav[0]/radials/target-auto-hdg-deg</property>
			<offset>
				<property>/orientation/heading-deg</property>
				<scale>-1.0</scale>
			</offset>
		</input>
		<output>/autopilot/internal/nav1-heading-error-deg</output>
		<period>
			<min>-180</min>
			<max>180</max>
		</period>
	</filter>
	
	<filter>
		<name>Internal Vertical Speed</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>/instrumentation/gps/indicated-vertical-speed</input>
		<output>/autopilot/internal/vert-speed-fpm</output>
	</filter>
	
	<filter>
		<name>Knots Rate</name>
		<type>derivative</type>
		<input>/velocities/airspeed-kt</input>
		<output>/autopilot/internal/kts-rate-raw</output>
		<filter-time>1.0</filter-time>
	</filter>
	
	<filter>
		<name>Knots Rate Lag</name>
		<type>exponential</type>
		<input>
			<condition>
				<less-than>
					<property>/velocities/airspeed-kt</property>
					<value>80</value>
				</less-than>
			</condition>
			<value>0</value>
		</input>
		<input>/autopilot/internal/kts-rate-raw</input>
		<output>/autopilot/internal/kts-rate</output>
		<filter-time>0.08</filter-time>
	</filter>
	
	<filter>
		<name>Flch Knots Rate Command</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<condition>
				<equals>
					<property>/instrumentation/afds/ap-modes/pitch-mode</property>
					<value>TO/GA</value>
				</equals>
			</condition>
			<expression>
				<product>
					<difference>
						<sum>
							<property>/instrumentation/weu/state/v2</property>
							<value>10</value>
						</sum>
						<property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
					</difference>
					<value>0.08</value>
				</product>
			</expression>
		</input>
		<input>
			<condition>
				<equals>
					<property>/instrumentation/afds/inputs/ias-mach-selected</property>
					<value>1</value>
				</equals>
			</condition>
			<expression>
				<product>
					<difference>
						<property>/autopilot/settings/target-speed-mach</property>
						<property>/instrumentation/airspeed-indicator/indicated-mach</property>
					</difference>
					<value>80</value>
				</product>
			</expression>
		</input>
		<input>
			<expression>
				<product>
					<difference>
						<property>/instrumentation/afds/ias-target-kt</property>
						<property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
					</difference>
					<value>0.08</value>
				</product>
			</expression>
		</input>
		<output>/autopilot/internal/flch-kts-error-cmd</output>
		<min>-1.5</min>
		<max>1.5</max>
	</filter>
	
	<filter>
		<name>Flch Knots Rate Filter</name>
		<type>noise-spike</type>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>/autopilot/internal/flch-kts-error-cmd</input>
		<output>/autopilot/internal/flch-kts-error</output>
		<max-rate-of-change>
			<condition>
				<and>
					<or>
						<equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>TO/GA</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>FLCH SPD</value>
						</equals>
					</or>
					<or>
						<property>/instrumentation/afds/inputs/AP</property>
						<property>/instrumentation/afds/inputs/FD</property>
					</or>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<value>2.5</value>
		</max-rate-of-change>
		<max-rate-of-change>100</max-rate-of-change>
	</filter>
	
	<filter>
		<name>Internal FPA Computer</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<difference>
					<property>/orientation/pitch-deg</property>
					<property>/orientation/alpha-deg</property>
				</difference>
			</expression>
		</input>
		<output>/autopilot/internal/fpa</output>
	</filter>
	
	<filter>
		<name>Nav G/S FPM Calc</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input> <!-- Under 110ft glideslope data starts to become unusable -->
			<condition>
				<and>
					<or>
						<equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>G/S</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>FLARE</value>
						</equals>
					</or>
					<less-than-equals>
						<property>/position/gear-agl-ft</property>
						<value>110</value>
					</less-than-equals>
				</and>
			</condition>
			<expression>
				<min>
					<property>/autopilot/internal/nav1-rate-of-climb-fpm</property>
					<value>-500</value>
				</min>
			</expression>
		</input>
		<input>/instrumentation/nav[0]/gs-rate-of-climb-fpm</input>
		<output>/autopilot/internal/nav1-rate-of-climb-fpm</output>
		<min>-1200</min>
		<max>0</max>
	</filter>
	
	<!-- Roll Axis -->
	<filter>
		<name>IT-CONTROLLER: Heading</name>
		<type>gain</type>
		<gain>
			<expression>
				<table>
					<property>/velocities/airspeed-kt</property>
					<entry><ind>140</ind><dep>1.6</dep></entry>
					<entry><ind>360</ind><dep>2.6</dep></entry>
				</table>
			</expression>
		</gain>
		<enable>
			<condition>
				<and>
					<or>
						<property>/instrumentation/afds/inputs/AP</property>
						<property>/instrumentation/afds/inputs/FD</property>
					</or>
					<or>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>HDG SEL</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>HDG HOLD</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>TRK SEL</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>TRK HOLD</value>
						</equals>
					</or>
				</and>
			</condition>
		</enable>
		<input>/autopilot/internal/heading-error-deg</input>
		<reference>0</reference>
		<output>/autopilot/internal/target-roll-deg</output>
		<min>/instrumentation/afds/settings/bank-min</min>
		<max>/instrumentation/afds/settings/bank-max</max>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: LNAV</name>
		<type>gain</type>
		<gain>1.75</gain>
		<enable>
			<condition>
				<and>
					<or>
						<property>/instrumentation/afds/inputs/AP</property>
						<property>/instrumentation/afds/inputs/FD</property>
					</or>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>LNAV</value>
					</equals>
                    <not-equals>
                        <property>/instrumentation/afds/ap-modes/lnav-is-pattern</property>
                        <value>1</value>
                    </not-equals>
				</and>
			</condition>
		</enable>
		<input>/autopilot/internal/course-error-deg</input>
		<reference>0</reference>
		<output>/autopilot/internal/target-roll-deg</output>
		<min>/instrumentation/afds/settings/bank-min</min>
		<max>/instrumentation/afds/settings/bank-max</max>
	</filter>
    
    <filter>
        <name>IT-CONTROLLER: LNAV Holding Pattern</name>
        <debug>false</debug>
        <type>gain</type>
        <gain>1.0</gain>
        <enable>
            <condition>
                <and>
                    <or>
                        <property>/instrumentation/afds/inputs/AP</property>
                        <property>/instrumentation/afds/inputs/FD</property>
                    </or>
                    <equals>
                        <property>/instrumentation/afds/ap-modes/roll-mode</property>
                        <value>LNAV</value>
                    </equals>
                    <equals>
                        <property>/instrumentation/afds/ap-modes/lnav-is-pattern</property>
                        <value>1</value>
                    </equals>
                </and>
            </condition>
        </enable>
        <input>/autopilot/internal/holding-pattern-roll</input>
        <output>/autopilot/internal/target-roll-deg</output>
        <min>-30</min>
        <max>30</max>
    </filter>
	
	<filter>
		<name>IT-CONTROLLER: VOR/LOC</name>
		<type>gain</type>
		<gain>
			<expression>
				<table>
					<property>/velocities/airspeed-kt</property>
					<entry><ind>140</ind><dep>1.6</dep></entry>
					<entry><ind>360</ind><dep>2.6</dep></entry>
				</table>
			</expression>
		</gain>
		<enable>
			<condition>
				<and>
					<or>
						<property>/instrumentation/afds/inputs/AP</property>
						<property>/instrumentation/afds/inputs/FD</property>
					</or>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>LOC</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>/autopilot/internal/nav1-heading-error-deg</input>
		<reference>0</reference>
		<output>/autopilot/internal/target-roll-deg</output>
		<min>/instrumentation/afds/settings/bank-min</min>
		<max>/instrumentation/afds/settings/bank-max</max>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: Rollout/Takeoff</name>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<and>
					<or>
						<property>/instrumentation/afds/inputs/AP</property>
						<property>/instrumentation/afds/inputs/FD</property>
					</or>
					<or>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>TO/GA</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>ROLLOUT</value>
						</equals>
					</or>
				</and>
			</condition>
		</enable>
		<input>0</input>
		<output>/autopilot/internal/target-roll-deg</output>
	</filter>
	
	<filter>
		<name>System Command: Roll Target Inactive Sync</name>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<and>
					<not><property>/instrumentation/afds/inputs/AP</property></not>
					<not><property>/instrumentation/afds/inputs/FD</property></not>
				</and>
			</condition>
		</enable>
		<input>/orientation/roll-deg</input>
		<output>/autopilot/internal/target-roll-deg</output>
		<min>/instrumentation/afds/settings/bank-min</min>
		<max>/instrumentation/afds/settings/bank-max</max>
	</filter>
	
	<filter>
		<name>System Command: Target Roll Filter</name>
		<input>/autopilot/internal/target-roll-deg</input>
		<output>/autopilot/internal/target-roll</output>
		<type>noise-spike</type>
		<max-rate-of-change>
			<condition>
				<and>
					<not><property>/instrumentation/afds/inputs/AP</property></not>
					<not><property>/instrumentation/afds/inputs/FD</property></not>
				</and>
			</condition>
			<value>1000</value>
		</max-rate-of-change>
		<max-rate-of-change>
			<condition>
				<not><property>/instrumentation/afds/inputs/AP</property></not>
			</condition>
			<value>15</value>
		</max-rate-of-change>
		<max-rate-of-change>6</max-rate-of-change>
	</filter>
	
	<filter>
		<name>System Command: Roll Difference</name>
		<type>gain</type>
		<gain>-0.9</gain>
		<enable>
			<condition>
				<property>/instrumentation/afds/inputs/AP</property>
			</condition>
		</enable>
		<input>/orientation/roll-deg</input>
		<reference>/autopilot/internal/target-roll</reference>
		<output>/autopilot/internal/roll-rate</output>
		<min>-5</min>
		<max>5</max>
	</filter>
	
	<pid-controller>
		<name>System Command: Aileron Air</name>
		<enable>
			<condition>
				<and>
					<property>/instrumentation/afds/inputs/AP</property>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>ROLLOUT</value>
					</not-equals>
					<or>
						<equals>
							<property>/gear/gear[1]/wow</property>
							<value>0</value>
						</equals>
						<equals>
							<property>/gear/gear[2]/wow</property>
							<value>0</value>
						</equals>
					</or>
				</and>
			</condition>
		</enable>
		<input>/orientation/roll-rate-degps</input>
		<reference>/autopilot/internal/roll-rate</reference>
		<output>/autopilot/internal/aileron-pid</output>
		<config>
			<Kp>
				<expression>
					<product>
						<table>
							<property>/velocities/airspeed-kt</property>
							<entry><ind>140</ind><dep>0.12</dep></entry>
							<entry><ind>360</ind><dep>0.04</dep></entry>
						</table>
						<table> <!-- Increase deflection if outer ailerons are locked -->
							<property>/fcs/logic/outer-aileron-locked</property>
							<entry><ind>0</ind><dep>1.0</dep></entry>
							<entry><ind>1</ind><dep>1.4</dep></entry>
						</table>
					</product>
				</expression>
			</Kp>
			<Ti>1.0</Ti>
			<Td>0.0001</Td>
			<u_min>-1.0</u_min>
			<u_max>1.0</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>System Command: Aileron Ground</name>
		<type>gain</type>
		<gain>-0.1</gain>
		<enable>
			<condition>
				<property>/instrumentation/afds/inputs/AP</property>
			</condition>
		</enable>
		<input>/orientation/roll-deg</input>
		<reference>0</reference>
		<output>/autopilot/internal/aileron-rollout</output>
	</filter>
	
	<filter>
		<name>System Command: Aileron Inactive Sync</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<not><property>/instrumentation/afds/inputs/AP</property></not>
			</condition>
		</enable>
		<input>/controls/flight/aileron</input>
		<output>/fcs/fbw/ap/roll-cmd</output>
	</filter>
	
	<filter>
		<name>System Command: Aileron Filter</name>
		<type>noise-spike</type>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<enable>
			<condition>
				<property>/instrumentation/afds/inputs/AP</property>
			</condition>
		</enable>
		<input>
			<condition>
				<and>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<property>/autopilot/internal/aileron-pid</property>
		</input>
		<input>/autopilot/internal/aileron-rollout</input>
		<output>/fcs/fbw/ap/roll-cmd</output>
		<max-rate-of-change>1.2</max-rate-of-change>
	</filter>
	
	<!-- Pitch Axis -->
	<!-- Altitude Hold Logic, "tries" to behave like IT-AUTOFLIGHT -->
	<logic>
		<input>
			<and>
				<or>
					<equals>
						<property>/instrumentation/afds/ap-modes/pitch-mode</property>
						<value>ALT</value>
					</equals>
					<and>
						<equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>VNAV PTH</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/inputs/vnav-path-mode</property>
							<value>0</value>
						</equals>
					</and>
					<equals>
						<property>/instrumentation/afds/ap-modes/pitch-mode</property>
						<value>VNAV ALT</value>
					</equals>
				</or>
				<or>
					<property>/instrumentation/afds/inputs/AP</property>
					<property>/instrumentation/afds/inputs/FD</property>
				</or>
			</and>
		</input>
		<output>/autopilot/internal/alt-active</output>
	</logic>
	
	<filter>
		<name>Altitude Error Abs</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<abs>
					<difference>
						<property>/autopilot/settings/actual-target-altitude-ft</property>
						<property>/instrumentation/altimeter/indicated-altitude-ft</property>
					</difference>
				</abs>
